{
  "name": "vacants-monorepo",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev:vacancies": "pnpm --filter vacancies dev",
    "dev:candidates": "pnpm --filter candidate-management dev",
    "build:vacancies": "pnpm --filter vacancies build",
    "build:candidates": "pnpm --filter candidate-management build",
    "prepare": "husky install"
  },
  "engineStrict": true,
  "browserslist": {
    "production": [
      ">0.25%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm test && pretty-quick --staged",
      "pre-push": "npm run lint && npm test"
    }
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.2",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.36.0",
    "@stylistic/eslint-plugin": "^3.1.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "eslint": "^9.36.0",
    "eslint-config-airbnb-extended": "^1.0.11",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import-x": "^4.15.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "install": "^0.13.0",
    "npx": "^10.2.2",
    "sass": "^1.93.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7",
    "vite-plugin-compression2": "^2.2.1",
    "vitest": "^3.2.4"
  }
}
